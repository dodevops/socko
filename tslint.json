{
  "extends": "tslint-config-standard",
  "rules": {
    "typedef": [
      true,
      "call-signature"
    ],
    "forin": true,
    "max-line-length": [
      true,
      140
    ],
    "member-access": true,
    "no-bitwise": true,
    "no-construct": true,
    "no-debugger": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-var-requires": true
  }
}